        $('#terms').click(function (evt) {
            evt.preventDefault();
            setup_modal('terms');
        });

        $('#privacy').click(function (evt) {
            evt.preventDefault();
            setup_modal('privacy');
        });

        $('.modal').on('show.bs.modal', function () {
            $('.modal .modal-body').css('max-height', $(window).height() * 0.7);
        });
    });

